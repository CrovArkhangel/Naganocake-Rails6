<h2>プロフィール編集</h2>
<%= form_with model: [:public, @receiver_address] do |f| %>
  <div class="form-group">
    <%= f.label :郵便番号（ハイフンなし） %>
    <%= f.text_field :zip_code, autofocus: true %>
  </div>
 <div class="form-group">
    <%= f.label :住所 %>
    <%= f.text_field :address %>
  </div>
  <div class="form-group">
    <%= f.label :宛名 %>
    <%= f.text_area :name %>
  </div>
  <%= f.submit "編集内容を保存" %>
<% end %>